	"jest": {
		"roots": [
			"<rootDir>/sso-tile-app",
			"<rootDir>/assets/js"
		],
		"testEnvironment": "jsdom",
		"moduleNameMapper": {
			"^.+\\.(css|less|scss)$": "babel-jest",
			".+/d3.min.js": "<rootDir>/node_modules/d3/dist/d3.min.js"
		},
		"transform": {
			"^.+\\.(js|jsx)$": "babel-jest"
		},
		"testPathIgnorePatterns": [
			"<rootDir>/dist",
			"<rootDir>/assets/coverage",
			"<rootDir>/sso-tile-app/coverage"
		],
		"setupFilesAfterEnv": [
			"<rootDir>/sso-tile-app/testSetupFile.js"
		],
		"collectCoverage": true,
		"collectCoverageFrom": [
			"assets/js/**/*.(js|jsx)",
			"sso-tile-app/src/**/*.(js|jsx)",
			"!**/.babelrc.js",
			"!sso-tile-app/src/reportWebVitals.js",
			"!sso-tile-app/src/index.js",
			"!**/.eslintrc.js",
			"!assets/js/NIPR/*",
			"!node_moduels/**/*"
		]
	}
}
