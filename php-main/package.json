{
	"scripts": {
		"nuke-dist": "rm -rf ./dist && mkdir ./dist && chmod 777 ./dist",
		"build": "npm run nuke-dist && npm --prefix ./sso-tile-app run build && npm --prefix ./assets run build",
		"test:unit": "jest --coverage",
		"test:one": "jest",
		"lint": "npm --prefix ./sso-tile-app run lint && npm --prefix ./assets run lint",
		"test:e2e-ci": "npx cypress run --spec cypress/integration/* --env baseUrl=https://competition.staging.dso.mil/ --config video=false,viewportWidth=1920,viewportHeight=1080,retries=1 --headless"
	},
